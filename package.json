    "start": "npm run dev"
  },
  "devDependencies": {
    "@picocss/pico": "2.0.6",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.71.1",
    "serve": "^14.2.1"
  },
  "browserslist": [
    "defaults"
  ]
}